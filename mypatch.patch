+                          <li>
+                            <a href={post.social[0].facebook} target="_blank" rel="noopener noreferrer">
+                              <i className="fab fa-facebook-f"></i>
+                            </a>
+                          </li>
+                        )}
+                        {post.social[0].twitter && (
